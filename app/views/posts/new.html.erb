<% breadcrumb :post_new %>

<h1>新規作成</h1>
<%= form_with model: @post, local: true do |f| %>
  <div>
    <%= f.label :name %>
  </div>
  <div>
    <%= f.text_field :name, placeholder: "名前を入力" %>
  </div>
  <div>
    <%= f.label :description %>
  </div>
  <div>
    <%= f.text_field :description, placeholder: "詳細を入力" %>
  </div>
  <%= f.submit nil %>
<% end %>
